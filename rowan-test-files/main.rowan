module Main;

class Main {
    @Override
    fn ready() {
        let x: u64 = 4 + 4;
        let printer: Printer = new Printer();
        printer.println-int(x);
        while x < 50 {
            printer.println-int(x);
            x = x + x;
        }
        printer.println-int(x);
        let arr: [u64] = [1, 2 ,3, 4];
        let counter: u64 = 0;
        while counter < arr.len() {
            printer.println-int(arr[counter]);
            counter = counter + 1;
        }
    }
}
