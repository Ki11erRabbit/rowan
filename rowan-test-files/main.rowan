module main;

import std::console::Console;

class Main {

    static thing: u64 = 66666666666;

    fn main(args: [String]) {
        let hello: String = "Hello, World!";
        Console::println(hello);
    }

    fn fib(n: u64) -> u64 {
        if n <= 1 {
            return n
        }
        return Main::fib(n - 1) + Main::fib(n - 2)
    }

    fn thrash() {
        let i: u64 = 0;
        while i < 10 {
            let x: [u64] = new [u64; 1];
            i = i + 1;
        }
    }

}
